{
  "appId": "com.friends.funtime",
  "appName": "Funtime Friends",
  "bundledWebRuntime": true,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    }
  },
  "linuxAndroidStudioPath": "studio",
  "server": {
    "url": "https://friends.funtime.com.ar",
    "allowNavigation": ["friends.funtime.com.ar"]
  },
  "android": {
    "overrideUserAgent": "funtime.android"
  },
  "ios": {
    "overrideUserAgent": "funtime.ios"
  }
}
